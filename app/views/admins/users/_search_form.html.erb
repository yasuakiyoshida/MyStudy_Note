<div class="row">
  <div class="col-lg-6 offset-lg-3 search-field text-center my-2">
    <%= search_form_for search, url: search_admins_users_path do |f| %>
      <%= f.search_field :nickname_cont, required: true, class: "search admin-search-form p-1 w-75", placeholder: 'ユーザー検索' %>
      <%= f.submit '検索', class: "search-btn p-1 admin-search-btn" %>
    <% end %>
  </div>
</div>